{
  "extends": "cheesecakelabs/react",
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "import/no-extraneous-dependencies": [
      "error",
      { "devDependencies": ["dist/**"] }
    ],
    "prettier/prettier": "error"
  },
  "globals": {},
  "plugins": ["react", "babel", "prettier"]
}